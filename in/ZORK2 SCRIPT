17585 (PUT CINT,C-TICK,TICK)
17590 (RETURN CINT)
17592 (*FUNCT INT,RTN,DEMON=0,E,C,INT)
17592 	(*PDEF INT)
17592 	(*BYTE 5)
17593 	(*WORD 0)
17595 	(*WORD 0)
17597 	(*WORD 0)
17599 	(*WORD 0)
17601 	(*WORD 0)
17603 (ADD C-TABLE,C-TABLELEN>E)
17607 (ADD C-TABLE,C-INTS>C)
17611 (?PRG1: EQUAL? C,E\?ELS5)
17616 (SUB C-INTS,C-INTLEN>C-INTS)
17620 (ZERO? DEMON/?ELS7)
17624 (SUB C-DEMONS,C-INTLEN>C-DEMONS)
17628 (?ELS7: ADD C-TABLE,C-INTS>INT)
17632 (PUT INT,C-RTN,RTN)
17637 (RETURN INT)
17639 (?ELS5: GET C,C-RTN)
